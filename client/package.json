{
	"name": "mymath-client",
	"version": "2.5.3",
	"private": true,
	"scripts": {
		"start": "dotenv -e .env.client.dev react-scripts start",
		"build-heroku": "dotenv -e .env.client.heroku react-scripts build",
		"build-prod": "dotenv -e .env.client.prod react-scripts build",
		"test": "react-scripts test",
		"cucumber-test": "node_modules/.bin/cucumber-js -f @cucumber/pretty-formatter ./src/test/features/CheckOutPurchaseCode.feature",
		"eject": "react-scripts eject",
		"lint": "eslint 'src/**/*.ts' && prettier --config ../.prettierrc.yml --list-different 'src/**/*.ts'",
		"lint:fix": "eslint --fix 'src/**/*.ts' && prettier --config ../.prettierrc.yml --write 'src/**/*.ts'"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@cucumber/cucumber": "^8.6.0",
		"@cucumber/pretty-formatter": "^1.0.0",
		"@nestjs/typeorm": "^9.0.1",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/crypto-js": "^4.1.0",
		"@types/jest": "^27.4.0",
		"@types/node": "^16.11.17",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"@types/react-helmet": "^6.1.6",
		"@types/react-router-dom": "^5.3.2",
		"@types/uuid": "^8.3.3",
		"axios": "^1.3.4",
		"chai": "^4.3.6",
		"crypto-js": "^4.1.1",
		"dotenv-cli": "^7.2.1",
		"gapi-script": "^1.2.0",
		"libphonenumber-js": "^1.9.44",
		"mariadb": "^3.1.0",
		"prettier": "^2.5.1",
		"react": "^17.0.2",
		"react-cookie": "^4.1.1",
		"react-dom": "^17.0.2",
		"react-google-login": "^5.2.2",
		"react-helmet": "^6.1.0",
		"react-router-dom": "^5.3.0",
		"react-scripts": "5.0.0",
		"sass": "^1.45.2",
		"selenium-webdriver": "^4.8.1",
		"semantic-ui-css": "2.5.0",
		"semantic-ui-react": "^2.0.4",
		"swiper": "^7.4.1",
		"swr": "^1.1.2",
		"typeorm": "^0.3.12",
		"typescript": "^4.5.4",
		"uuid": "^8.3.2",
		"web-vitals": "^2.1.2"
	},
	"resolutions": {
		"@types/react": "17.0.2",
		"@types/react-dom": "17.0.2"
	}
